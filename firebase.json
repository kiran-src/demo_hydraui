{
    "database": {
        "rules": "database.rules.json"
    },
    "hosting": {
        "site": "qot-hyperautomation",
        "public": "dist/qot/browser",
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    },
    "firestore": {
        "rules": "firestore.rules",
        "indexes": "firestore.indexes.json"
    }
}
