{
    "name": "qot",
    "version": "1.2.0",
    "scripts": {
        "ng": "ng",
        "start": "ts-node server.ts",
        "build-deploy": "npm run build && npm run deploy && npm run git",
        "git": "git add . && git commit -m 'setup pack.json' && git push",
        "build": "npm install -g @angular/cli && ng build --configuration production --aot --output-hashing=all --optimization=true",
        "deploy": "firebase deploy",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "clean": "Remove-Item -Recurse -Force node_modules, dist, .angular",
        "serve:ssr:qot": "node dist/qot/server/server.mjs",
        "build:prod": "ng build --configuration production --base-href /",
        "postbuild:prod": "cd dist && zip -r ../dist.zip .",
        "heroku-postbuild": "ng build --prod"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.2.0",
        "@angular/cdk": "^17.2.1",
        "@angular/cli": "^17.2.2",
        "@angular/common": "^17.2.0",
        "@angular/compiler": "^17.2.0",
        "@angular/compiler-cli": "^17.2.0",
        "@angular/core": "^17.2.0",
        "@angular/fire": "^17.1.0",
        "@angular/forms": "^17.2.0",
        "@angular/material": "^17.2.1",
        "@angular/platform-browser": "^17.2.0",
        "@angular/platform-browser-dynamic": "^17.2.0",
        "@angular/platform-server": "^17.2.0",
        "@angular/router": "^17.2.0",
        "@angular/ssr": "^17.2.2",
        "@fullcalendar/angular": "^6.1.10",
        "@fullcalendar/core": "^6.1.10",
        "@fullcalendar/daygrid": "^6.1.10",
        "@iplab/ngx-file-upload": "^17.1.0",
        "@stomp/stompjs": "^7.0.0",
        "@types/d3-array": "^3.2.1",
        "@types/d3-scale": "^4.0.8",
        "@types/d3-selection": "^3.0.11",
        "@types/d3-shape": "^3.1.6",
        "@types/xlsx": "^0.0.35",
        "apexcharts": "^3.35.0",
        "d3": "^7.0.0",
        "express": "^4.21.2",
        "firebase": "^10.12.2",
        "jspdf": "^2.5.2",
        "moment": "^2.30.1",
        "ng-apexcharts": "^1.7.1",
        "ngx-color-picker": "^16.0.0",
        "ngx-editor": "^17.0.2",
        "ngx-gauge": "^9.0.0",
        "ngx-mat-timepicker": "^17.1.0",
        "ngx-owl-carousel-o": "^17.0.0",
        "ngx-scrollbar": "^14.1.0",
        "path": "^0.12.7",
        "qot": "file:",
        "remixicon": "^4.2.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "typescript": "~5.3.2",
        "xlsx": "^0.18.5",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.3.8",
        "@ionic/angular-toolkit": "^12.1.1",
        "@types/d3": "^7.4.3",
        "@types/express": "^4.17.17",
        "@types/jasmine": "~5.1.0",
        "@types/node": "^18.19.75",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "ts-node": "^10.9.2"
    },
    "engines": {
        "node": "20.18.2",
        "npm": "10.9.0"
    }
}
